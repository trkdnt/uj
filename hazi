                "SELECT t.Tantárgynév, COUNT(*) AS frequency\n",
                "FROM orak o JOIN Tantárgyak t on o.targy=t.Tkód\n",
                "GROUP BY t.Tantárgynév\n",
                "ORDER BY frequency DESC"

        },
        {
            "cell_type": "code",
            "source": [
                "SELECT Tantárgynév,  \n",
                "COUNT(*) AS gyakoriság,\n",
                "(COUNT(*) * 100 / (SELECT COUNT(*) FROM Orak o Join Tantárgyak t on o.targy=t.Tkód)) AS százalék \n",
                "FROM Orak o Join Tantárgyak t on o.targy=t.Tkód\n",
                "GROUP BY Tantárgynév\n",
                "ORDER BY gyakoriság DESC;\n",
                ""
            ],
